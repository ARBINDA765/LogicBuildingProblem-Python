   "source": [
    "def divis_fun(numbers,divisors):\n",
    "    divisible = []\n",
    "    for num in numbers:\n",
    "        if num%divisors==0:\n",
    "            divisible.append(num)\n",
    "    return divisible\n",
    "            "
   ]
