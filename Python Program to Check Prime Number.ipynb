   "source": [
    "def is_prime(num):\n",
    "    if num<=1:\n",
    "        return False\n",
    "    for i in range(2,int(num**.5)+1):\n",
    "        if num%i==0:\n",
    "            return False\n",
    "    return True "
   ]
